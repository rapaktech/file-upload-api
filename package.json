{
  "name": "risevest-backend-test",
  "version": "1.0.0",
  "description": "Backend Test For Risevest",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "test": "jest src",
    "postinstall": "tsc",
    "watch-node": "nodemon dist/src/index.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku main"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "keywords": [
    "example",
    "heroku"
  ],
  "author": "Jim Ezesinachi",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.11",
    "@types/node": "^17.0.18",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.9.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "aws-sdk": "^2.1074.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "pg": "^8.7.3",
    "xss": "^1.0.10",
    "xss-clean": "^0.1.1"
  },
  "engines": {
    "node": "14.x"
  }
}
